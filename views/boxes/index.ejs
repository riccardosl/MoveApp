<h2>Search your box</h2>
<form action="/boxes" method="GET">
  <div>
  <label>Name</label>
  <input type="text" name="title" value="<%= searchOptions.title %>" >
  </div>
  <div>
  <label>Date</label>
  <input type="date" name="addedDate" value="<%= searchOptions.addedDate %>" >
  </div>
  <button type="submit">Search</button>
</form>
<div>



<div>
  <% boxes.forEach(box => { %>
    <img height="150" width="100" src="<%= box.coverImagePath %>">
  <% }) %>
</div>
